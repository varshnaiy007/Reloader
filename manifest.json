{
    "manifest_version": 3,
    "name": "Auto Refresh v3",
    "version": "1.0",
    "description": "Refresh the page every 10 seconds.",
    "permissions": ["tabs"],
    "background": {
      "service_worker": "background_script.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
}
  